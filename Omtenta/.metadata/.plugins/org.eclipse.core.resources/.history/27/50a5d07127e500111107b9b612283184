import java.util.Scanner;

public class sdfg {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int value = scan.nextInt();
		String s=null;
		if (value > 1) {
			double sqrt = Math.sqrt(value);
			for (int i = 0; i <= sqrt; i++) {
				if (value % i == 0) {
					s = "false";
				}
			}
			s="true";
		}
		System.out.println(s);
	}
	
}
